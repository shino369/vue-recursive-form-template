    "moment": "^2.29.4",
    "pinia": "^2.1.4",
    "vee-validate": "^4.11.1",
    "vue": "^3.2.45",
    "vue-i18n": "^9.3.0-beta.14-77e850b",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.12.2",
